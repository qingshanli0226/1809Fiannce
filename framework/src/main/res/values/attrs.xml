<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PregressMyView">
        <attr name="textColor" format="color"></attr>
        <attr name="circleWidth" format="integer"></attr>
        <attr name="background" format="reference"></attr>
        <attr name="upCirclecolor" format="color"></attr>
        <attr name="downCirclecolor" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="ToolBar">
        <attr name="titleText" format="string"></attr>
        <attr name="rightImage" format="reference"></attr>
        <attr name="rightIsShow" format="boolean"></attr>
        <attr name="leftImage" format="reference"></attr>
        <attr name="leftIsShow" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="MoreItem">
        <attr name="moreRightText" format="string"></attr>
        <attr name="moreRightImage" format="reference"></attr>
        <attr name="moreRightIsShow" format="boolean"></attr>
        <attr name="moreLeftText" format="string"></attr>
        <attr name="moreLeftImage" format="reference"></attr>
        <attr name="moreLeftIsShow" format="boolean"></attr>
    </declare-styleable>
</resources>