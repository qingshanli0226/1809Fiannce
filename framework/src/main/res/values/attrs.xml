<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ProgressView">

        <attr name="TextColor" format="color"></attr>
        <attr name="TextSize" format="integer"></attr>
        <attr name="PaintSize" format="integer"></attr>
        <attr name="PaintColor1" format="color"></attr>
        <attr name="PaintColor2" format="color"></attr>

    </declare-styleable>

    <declare-styleable name="ToolBar">

        <attr name="titleText" format="string"></attr>
        <attr name="rightImage" format="reference"></attr>
        <attr name="rightIsShow" format="boolean"></attr>
        <attr name="leftImage" format="reference"></attr>
        <attr name="leftIsShow" format="boolean"></attr>

    </declare-styleable>

</resources>