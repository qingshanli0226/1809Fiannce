<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/gray"
    tools:context=".mine.MineFragment">


    <com.example.framework.view.ToolBar
        android:id="@+id/toolbar"
        app:app_text="@string/mian_money"
        app:app_back_color_toob="@color/mine_black"
        app:app_text_color="@color/blue"
        app:app_right_src="@drawable/my_setting_icon"
        app:app_right_isshow="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <com.example.framework.view.ItemBar
        app:app_item_text="aaaa"
        app:app_item_back_color="@color/white"
        app:app_item_left_src="@drawable/my_invest_manage"
        app:app_item_left_isshow="true"
        app:app_item_right_src="@drawable/right"
        app:app_item_right_isshow="true"
        android:layout_marginTop="100dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</FrameLayout>