<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".Fragment.oneFragment">
    <com.fiance.framework.MyView.ToolBar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        app:rightIsShow="true"
        app:leftIsShow="false"
        app:titleText="首页"
        android:layout_height="60dp">
    </com.fiance.framework.MyView.ToolBar>
    <com.youth.banner.Banner
        android:layout_width="match_parent"
        android:id="@+id/banner"
        android:layout_height="200dp"></com.youth.banner.Banner>

    <com.fiance.framework.MyView.MyView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginLeft="120dp"
        android:id="@+id/myview"></com.fiance.framework.MyView.MyView>


</LinearLayout>