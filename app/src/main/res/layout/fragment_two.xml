<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".Fragment.twoFragment">
    <com.fiance.framework.MyView.ToolBar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        app:rightIsShow="true"
        app:leftIsShow="false"
        app:titleText="投资"
        android:layout_height="60dp">
    </com.fiance.framework.MyView.ToolBar>
    <com.google.android.material.tabs.TabLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tab"></com.google.android.material.tabs.TabLayout>

    <androidx.viewpager.widget.ViewPager
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/vp"></androidx.viewpager.widget.ViewPager>


</LinearLayout>