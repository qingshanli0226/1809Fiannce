<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context="com.finance.zg6.mianInsideFragment.MoreFragment">
    <com.finance.framework.view.ToolBar
        android:layout_width="match_parent"
        android:layout_height="@dimen/ToolBar_height"
        app:leftImg="@mipmap/left"
        app:leftIsShow="false"
        app:titleText="@string/mainActivity_fragment_title_more"></com.finance.framework.view.ToolBar>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/more_RelativeLayout_height"
        android:layout_marginLeft="@dimen/margin"
        android:layout_marginRight="@dimen/margin"
        android:id="@+id/iconMoreRegist">
        <ImageView
            android:layout_width="@dimen/more_img_width"
            android:layout_height="@dimen/more_img_height"
            android:src="@drawable/icon_more_regist"
            android:layout_centerVertical="true"
            android:id="@+id/icon_more_regist"></ImageView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/more_text_size"
            android:text="@string/icon_more_regist"
            android:textColor="@color/gray_blue"
            android:layout_toRightOf="@+id/icon_more_regist"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin"></TextView>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_height"
        android:background="@color/grey"></View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/more_RelativeLayout_height"
        android:layout_marginLeft="@dimen/margin"
        android:layout_marginRight="@dimen/margin"
        android:id="@+id/iconMoreSecret">
        <ImageView
            android:layout_width="@dimen/more_img_width"
            android:layout_height="@dimen/more_img_height"
            android:src="@drawable/icon_more_secret"
            android:layout_centerVertical="true"
            android:id="@+id/icon_more_secret"></ImageView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/more_text_size"
            android:text="@string/icon_more_secret"
            android:textColor="@color/gray_blue"
            android:layout_toRightOf="@+id/icon_more_secret"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin"></TextView>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/toggle_off"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/toggleOff"></ImageView>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_height"
        android:background="@color/grey"></View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/more_RelativeLayout_height"
        android:layout_marginLeft="@dimen/margin"
        android:layout_marginRight="@dimen/margin"
        android:id="@+id/iconMoreReset">
        <ImageView
            android:layout_width="@dimen/more_img_width"
            android:layout_height="@dimen/more_img_height"
            android:src="@drawable/icon_more_reset"
            android:layout_centerVertical="true"
            android:id="@+id/icon_more_reset"></ImageView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/more_text_size"
            android:text="@string/icon_more_reset"
            android:textColor="@color/gray_blue"
            android:layout_toRightOf="@+id/icon_more_reset"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin"></TextView>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_height"
        android:background="@color/grey"></View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/more_RelativeLayout_height"
        android:layout_marginLeft="@dimen/margin"
        android:layout_marginRight="@dimen/margin"
        android:id="@+id/iconMoreContact">
        <ImageView
            android:layout_width="@dimen/more_img_width"
            android:layout_height="@dimen/more_img_height"
            android:src="@drawable/icon_more_contact"
            android:layout_centerVertical="true"
            android:id="@+id/icon_more_contact"></ImageView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/more_text_size"
            android:text="@string/icon_more_contact"
            android:textColor="@color/gray_blue"
            android:layout_toRightOf="@+id/icon_more_contact"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin"></TextView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/text_mid_size"
            android:textColor="@color/colorRed"
            android:text="@string/more_tel"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"></TextView>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_height"
        android:background="@color/grey"></View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/more_RelativeLayout_height"
        android:layout_marginLeft="@dimen/margin"
        android:layout_marginRight="@dimen/margin"
        android:id="@+id/iconMoreSms">
        <ImageView
            android:layout_width="@dimen/more_img_width"
            android:layout_height="@dimen/more_img_height"
            android:src="@drawable/icon_more_sms"
            android:layout_centerVertical="true"
            android:id="@+id/icon_more_sms"></ImageView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/more_text_size"
            android:text="@string/icon_more_sms"
            android:textColor="@color/gray_blue"
            android:layout_toRightOf="@+id/icon_more_sms"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin"></TextView>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_height"
        android:background="@color/grey"></View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/more_RelativeLayout_height"
        android:layout_marginLeft="@dimen/margin"
        android:layout_marginRight="@dimen/margin"
        android:id="@+id/iconMoreShare">
        <ImageView
            android:layout_width="@dimen/more_img_width"
            android:layout_height="@dimen/more_img_height"
            android:src="@drawable/icon_more_share"
            android:layout_centerVertical="true"
            android:id="@+id/icon_more_share"></ImageView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/more_text_size"
            android:text="@string/icon_more_regist"
            android:textColor="@color/gray_blue"
            android:layout_toRightOf="@+id/icon_more_share"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin"></TextView>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_height"
        android:background="@color/grey"></View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/more_RelativeLayout_height"
        android:layout_marginLeft="@dimen/margin"
        android:layout_marginRight="@dimen/margin"
        android:id="@+id/iconMoreAbout">
        <ImageView
            android:layout_width="@dimen/more_img_width"
            android:layout_height="@dimen/more_img_height"
            android:src="@drawable/icon_more_about"
            android:layout_centerVertical="true"
            android:id="@+id/icon_more_about"></ImageView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/more_text_size"
            android:text="@string/icon_more_about"
            android:textColor="@color/gray_blue"
            android:layout_toRightOf="@+id/icon_more_about"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/margin"></TextView>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_height"></View>
</LinearLayout>