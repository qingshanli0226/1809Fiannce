<?xml version="1.0" encoding="utf-8"?>
<com.fiannce.bawei.a1809fiannce.event.MyRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.fiannce.bawei.framework.view.ToolBar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        app:rightIsShow="true"
        app:rightImage="@mipmap/mh"
        app:leftImg="@mipmap/mh"
        app:leftIsShow="false"
        app:titleText="首页"
        android:layout_height="60dp">
    </com.fiannce.bawei.framework.view.ToolBar>

    <TextView
        android:id="@+id/progressTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        />

    <com.fiannce.bawei.framework.view.ProgressView
        android:id="@+id/progressView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:textColor="@color/colorPrimary"
        app:circleWith="10"
        app:background="@mipmap/mh"
        android:layout_centerInParent="true"/>

</com.fiannce.bawei.a1809fiannce.event.MyRelativeLayout>