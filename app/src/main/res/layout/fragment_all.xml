<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.fiannce.bawei.fragment.investFragment.tabFragment.allFragment.AllFragment">

    <com.fiannce.framework.view.ToolBar
        android:layout_width="match_parent"
        android:id="@+id/toolbar"
        android:visibility="gone"
        android:layout_height="@dimen/fourty"></com.fiannce.framework.view.ToolBar>

    <com.fiannce.framework.view.MyTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorWhite"
        android:text="@string/marquee"
        android:textColor="@color/colorRedqian"
        android:singleLine="true"
        android:ellipsize="marquee"
        android:textSize="@dimen/twenty_five"
        ></com.fiannce.framework.view.MyTextView>

    <com.fiannce.framework.view.SlideRecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/allRv"
        >
    </com.fiannce.framework.view.SlideRecyclerView>

</LinearLayout>